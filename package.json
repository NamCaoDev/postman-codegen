{
  "name": "@namcaodev/postman-codegen",
  "author": "Nam Cao Dev",
  "version": "0.0.6",
  "license": "MIT",
  "homepage": "https://github.com/NamCaoDev/postman-codegen.git#readme",
  "description": "Auto generate all types, query options, mutation options by postman json",
  "main": "index.js",
  "files": [
    "index.ts"
  ],
  "bin": {
    "postman-codegen": "bin/codegen.js"
  },
  "private": false,
  "keywords": [
    "codegen",
    "tanstack-query",
    "postman-json",
    "auto-generate-type",
    "plop",
    "quicktype",
    "typescript"
  ],
  "repository": {
    "url": "git+https://github.com/NamCaoDev/postman-codegen.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/NamCaoDev/postman-codegen.git/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "codegen-ts": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=index.ts generate-queries",
    "codegen": "npx plop --plopfile=dist/index.js generate-queries"
  },
  "dependencies": {
    "@turbo/gen": "^2.4.4",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.3.3",
    "lodash": "^4.17.21",
    "p-limit": "^6.2.0",
    "quicktype": "^23.0.171",
    "quicktype-core": "^23.0.171",
    "ts-node": "^10.9.2",
    "ts-to-zod": "^3.15.0",
    "plop": "^4.0.1",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "node-plop": "^0.32.0",
    "typescript": "~5.8.2"
  }
}
